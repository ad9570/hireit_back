<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.InsContentMapper">
    <select id="getInsContents" parameterType="int" resultType="insightContent">
        select * 
        from career_content
        where tag = #{tag}
    </select>
</mapper>